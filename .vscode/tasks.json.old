{
    "version": "0.1.0",
    "command": "npm",
    "isShellCommand": true,
    "suppressTaskName": true,
    "tasks": [
        {
            // Build task, Cmd+Shift+B
            // "npm run build"
            "taskName": "build",
            "isBuildCommand": true,
            "args": [
                "run",
                "build"
            ]
        },
        {
            // Test task, Cmd+Shift+T
            // "npm test"
            "taskName": "test",
            "isTestCommand": true,
            "args": [
                "test"
            ]
        }
    ]
}
